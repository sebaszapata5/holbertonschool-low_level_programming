#include "holberton.h"
/**
 * swap_int - check the code for Holberton School students.
 * @s: variable s
 * Return: Always 0.
 */
void rev_string(char *s)
{
	char t;
	char l = s;
	char e = &(l[strlen(l)-1]);
	while (l < e)
{
		t = l;
		l++ = e;
		e-- = t;
	}
}

